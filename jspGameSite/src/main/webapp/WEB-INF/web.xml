<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>jspGameSite</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>index</servlet-name>
  	<jsp-file>/WEB-INF/index.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>index</servlet-name>
  	<url-pattern>/index</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>login</servlet-name>
  	<jsp-file>/WEB-INF/login.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>register</servlet-name>
  	<jsp-file>/WEB-INF/register.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>register</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>notice</servlet-name>
  	<jsp-file>/WEB-INF/news/notice.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>notice</servlet-name>
  	<url-pattern>/news/notice</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>noticeDetail</servlet-name>
  	<jsp-file>/WEB-INF/news/noticeDetail.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>noticeDetail</servlet-name>
  	<url-pattern>/news/noticeDetail</url-pattern>
  </servlet-mapping>
  
  
</web-app>